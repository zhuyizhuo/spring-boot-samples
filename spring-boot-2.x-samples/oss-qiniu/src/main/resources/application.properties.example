# Qiniu Cloud OSS Configuration
# Please copy this file to application.properties and fill in your actual credentials

# Qiniu Cloud Access Key
qiniu.oss.access-key=your_access_key_here

# Qiniu Cloud Secret Key
qiniu.oss.secret-key=your_secret_key_here

# Qiniu Cloud Bucket Name\qiniu.oss.bucket-name=your_bucket_name_here

# Qiniu Cloud Domain (CDN domain or bucket domain)
qiniu.oss.domain=https://your_domain.com

# Qiniu Cloud Region
# Available regions: huadong, huabei, huanan, beimei, xinjiapo
qiniu.oss.region=huadong

# Whether to use HTTPS
qiniu.oss.use-https=true

# Server Configuration
server.port=8083
server.servlet.context-path=/oss-qiniu

# Log Configuration
logging.level.root=INFO
logging.level.com.github.zhuyizhuo.sample.oss=DEBUG

# Spring Configuration
spring.application.name=oss-qiniu-sample